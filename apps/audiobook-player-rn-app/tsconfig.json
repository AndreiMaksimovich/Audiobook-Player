{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "outDir": "dist",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "lib": ["DOM", "ESNext", "WebWorker"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
